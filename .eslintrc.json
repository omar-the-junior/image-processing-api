{
  "env": {
    "browser": false,
    "node": true,
    "commonjs": true,
    "jasmine": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "overrides": [{
    "plugins": ["plugin:@typescript-eslint/recommended"],
    "files" : ["**.js"]
  }],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "warn"
  }
}
